<div class="form-group">
  <label for="name">Name</label>
  <input type="text"
         class="form-control"
         id="name"
         name="name"
         value="<%= food.name %>">
</div>

<div class="form-group">
  <label for="address">Address</label>
  <input type="text"
         class="form-control"
         id="address"
         name="address"
         value="<%= food.address %>">
</div>

<div class="form-group">
  <label for="date">Date</label>
  <input type="date"
         class="form-control"
         id="date"
         date="date"
         value="<%= food.date %>">
</div>

<div class="form-group">
  <label for="Time">Time</label>
  <input type="date"
         class="form-control"
         id="Time"
         Time="Time"
         value="<%= food.Time %>">
</div>
